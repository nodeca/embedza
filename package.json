{
  "name": "embedza",
  "version": "2.2.0",
  "description": "Create HTML snippets/embeds from URLs using info from oEmbed, Open Graph, meta tags.",
  "keywords": [
    "youtube",
    "vimeo",
    "oembed",
    "opengraph"
  ],
  "repository": "nodeca/embedza",
  "license": "MIT",
  "files": [
    "assets/",
    "config/generic.yml",
    "lib/",
    "support/",
    "index.js"
  ],
  "dependencies": {
    "bluebird": "^3.4.6",
    "cheerio": "^0.22.0",
    "debug": "^2.2.0",
    "got": "^7.0.0",
    "js-yaml": "^3.3.1",
    "lodash": "^4.0.0",
    "probe-image-size": "^3.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "eslint": "^3.9.1",
    "express": "^4.13.1",
    "istanbul": "^0.4.1",
    "mocha": "^3.1.2",
    "nock": "^9.0.2",
    "proclaim": "^3.4.4",
    "pug": "^2.0.0-beta6",
    "supertest": "^3.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && mocha",
    "coverage": "rm -rf coverage && istanbul cover _mocha",
    "report-coveralls": "istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls.js && rm -rf ./coverage",
    "install": "node support/install",
    "start": "node support/server",
    "heroku-postbuild": "npm install express pug"
  }
}
